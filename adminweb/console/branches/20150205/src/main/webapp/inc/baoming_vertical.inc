                                <input type="text" name="carStyle" class="inpt hide" autocomplete="off">
                                <span class="help-inline"></span>
                                <div class="select_selected">
                                    <span class="select_val">选择车型</span>
                                    <i class="down_arr"></i>
                                </div>
                                <div class="select_con select_style"></div>
                            </div>
                        </div>
                    </div>
                    <div class="form_control">
                        <label class="f_label">手机</label>
                        <div class="controls">
                          	  <input  type="hidden" name="url" id="url" value="${specialSubject.encodeSpUrl}"/>
                             <input type="text" name="phone" class="inpt" maxlength="11" placeholder="请输入购车人手机号" >
                             <span class="help-inline"></span>
                        </div>
                    </div>
                    <div class="form_control">
                        <label class="f_label">&nbsp;</label>
                        <div class="controls">
                               <button type="submit" class="g-btn g-btn-l g-btn-sure"><span>团购报名</span></button>
